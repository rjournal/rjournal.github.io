@misc{pwr2018,
  title = "pwr: Basic functions for power analysis",
  abstract = "This package contains functions for basic power calculations using effect sizes and notations from Cohen (1988): pwr.p.test: test for one proportion (ES=h) pwr.2p.test: test for two proportions (ES=h) pwr.2p2n.test: test for two proportions (ES=h, unequal sample sizes) pwr.t.test: one sample and two samples (equal sizes) t tests for means (ES=d) pwr.t2n.test: two samples (different sizes) t test for means (ES=d) pwr.anova.test: test for one-way balanced anova (ES=f) pwr.r.test: correlation test (ES=r) pwr.chisq.test: chi-squared test (ES=w) pwr.f2.test: test for the general linear model (ES=f2) ES.h: computing effect size h for proportions tests ES.w1: computing effect size w fort he goodness of fit chi-squared test ES.w2: computing effect size w for the association chi-squared test cohen.ES: computing effect sizes for all the previous tests corresponding to conventional effect sizes (small, medium, large).",
  keywords = "R, Power analysis",
  author = "Stephane Champely and Claus Ekstrom and Peter Dalgaard and Jeffrey Gill and Stephan Weibelzahl and Aditya Anandkumar and Clay Ford and Robert Volcic and {De Rosario}, Helios",
  year = "2017",
  language = "English (US)",
  url = {https://cran.r-project.org/web/packages/pwr/index.html}
}

