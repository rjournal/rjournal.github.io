	Doi = {http://dx.doi.org/10.1016/j.fishres.2015.03.011},
	Issn = {0165-7836},
	Journal = {Fisheries Research},
	Keywords = {Marine protected areas},
	Pages = {313 - 322},
	Title = {Spawning habitat selection by Octopus vulgaris: New insights for a more effective management of this resource},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165783615001010},
	Volume = {167},
