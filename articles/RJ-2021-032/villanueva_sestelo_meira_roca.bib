@article{RefWorks:10,
	Abstract = {Pollicipes pollicipes is widely used as food and is the subject of intensive fisheries in Portugal. Since November 1990 growth of this species was studied on the SW coast of Portugal. Two kinds of methods were used: (1) individual growth: the adjacent substrata were marked with polyester resin and the organisms were mapped and measured (Rostro-Carina length=RC) every month; 2) spatial variability of allometric growth: various biometrical measures were used for animals from different habitats; the relationship between these values and the organic weight were established. The results of these analyses suggest that there are spatial (due to tide and light related factors) and temporal differences (growth rates are higher in spring and summer) in the growth of this pedunculate barnacle. In this study RC was the biometrical variable that best represented the linear growth of this species and we recommended its use for fishery control purposes.},
	Author = {T. Cruz},
	Date-Added = {2009-05-28 18:17:51 +0200},
	Date-Modified = {2010-05-25 12:17:06 +0200},
	Isbn = {0011-216X},
	Journal = {Crustaceana},
	Keywords = {Article Subject Terms: crustacean fisheries; environmental conditions; growth; seasonal variations; spatial variations; temporal variations; Article Taxonomic Terms: Cirripedia; Pollicipes pollicipes; Article Geographic Terms: ANE, Portugal; Portugal; crustacean fisheries; Marine},
	Language = {English; English; French},
