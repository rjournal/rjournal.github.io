	Journal = {Marine Ecology Progress Series},
	Keywords = {Article Subject Terms: Circadian rhythms; Congeners; Crustacean larvae; Daytime; Environmental effects; High tide; Intertidal environment; Marine crustaceans; Moon; Moon phases; Predators; Reproductive behaviour; Rocky shores; Shores; Spawning; Survival; Tidal cycles; Tidal effects; Tides; Water column; Article Taxonomic Terms: Balanus; Chthamalus; Cirripedia; Pollicipes; Pollicipes pollicipes; Article Geographic Terms: ANE, Spain, Galicia; Spain; Barnacles; Marine},
	Language = {English; English},
