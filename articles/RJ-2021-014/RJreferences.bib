@article{hargartenAccountingUncertaintyDue2019,
  title = {Accounting for the {{Uncertainty}} Due to {{Chemicals Below}} the {{Detection Limit}} in {{Mixture Analysis}}},
  abstract = {Simultaneous exposure to a mixture of chemicals over a lifetime may increase an individual's risk of disease. Weighted quantile sum (WQS) regression estimates the effect of exposure to chemicals and identifies the important, or the key etiologically-relevant, components in the mixture. However, complications arise when experimental apparatus can only detect chemical concentrations to a detection limit. Strategies to account for the values below the detection limit (BDL) in WQS analyses are limiting. Researchers may place BDLs into the first quantile of the weighted index (BDLQ1) or perform bootstrapping or Bayesian imputation. We propose integrating WQS regression into the multiple imputation (MI) framework and to compare these approaches via a simulation study over a range of values BDL. We examined the ability of each method to estimate the mixture effect and to identify important chemicals.  As expected, as the number of values BDL increases, the accuracy, precision, model fit, and power decline. When chemical values are missing at 10\%, 33\%, or 50\%, imputation methods performed better than single imputation and BDLQ1; however, when 80\% of the chemical values are missing, the BDLQ1 approach is the best.},
  journaltitle = {Manuscript in Preparation},
  series = {Manuscript in Preparation.},
  date = {2019},
  keywords = {WQS,vignette,Chemical Mixture},
  author = {Hargarten, Paul M. and Wheeler, David C.}
}

