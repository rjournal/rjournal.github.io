@software{hargartenMiWQSMultipleImputation2018,
  title = {{{miWQS}}: {{Multiple Imputation Using Weighted Quantile Sum Regression}}},
  url = {https://CRAN.R-project.org/package=miWQS},
  abstract = {Package created for Aim 2-- MI. 

 Consider c correlated components reasonable to combine into an index but are interval-censored between 0 and some positive number (henceforth, the detection limit). The miWQS package applies the multiple imputation (MI) procedure to the weighted quantile sum regression (WQS) methodology for continuous, binary, or count outcomes.  In summary, MI consists of three stages: (1) imputation, (2) analysis, and (3) pooling. First,  the missing values are imputed by bootstrapping (Lubin et.al (2004) {$<$}doi:10.1289/ehp.7199{$>$}) , Bayesian imputation, or placing the below the detection limits in the first quantile (BDLQ1) (Ward et.al. (2014) {$<$}doi:10.1289/ehp.1307602{$>$}) . Second, the estimate.wqs() implements WQS regression if the components are complete, imputed, or missing (Carrico et.al. (2014) {$<$}doi:10.1007/s13253-014-0180-3{$>$}) . If the data is missing, BDLQ1 is automatically implemented.  Lastly, the pool.mi() calculates the pooled statistics according to Rubin's rules (Rubin 1987).},
  version = {0.0.9},
  date = {2018},
  author = {Hargarten, Paul M. and Wheeler, David C.}
}

