@techreport{bolksBaselineAssessmentLeftCensored2014,
  location = {{Fairfax, VA}},
  title = {Baseline {{Assessment}} of {{Left}}-{{Censored Environmental Data Using R}}},
  url = {www.bae.ncsu.edu/programs/extension/wqg/319monitoring/tech_notes.htm},
  abstract = {The entrepreneurial nature of most watershed managers is to analyze their own data.  This can unfortunately lead to data analyses that are not complete or statistically robust. This might occur when a watershed manager is computing summary statistics (e.g., mean, median, quartiles) from a set of water quality samples containing observations reported as less than ({$<$}) or not detected (ND). To avoid this trap, watershed managers can turn to R (R Core Team 2013), a freely available software tool from the Comprehensive R Archive Network (CRAN).  

This Tech Note provides
 (1) instructions for downloading and installing R and RStudio;
(2) a brief introduction to R; and 
(3) a method for robustly computing and displaying summary statistics (e.g., mean, median, quartiles) from monitoring program data with less-thans or non-detects using robust regression on order statistics (robust ROS) and maximum likelihood estimation (MLE).

As R's command line is difficult to navitage, we have provided a sample data set and script to compute summary statistics and generate graphics. We hope these scripts will encourage nonpoint source watershed managers to apply these tools to their water quality programs.},
  series = {Tech {{Notes}} 10},
  institution = {{Tetra Tech, Inc.,}},
  date = {2014-06},
  pages = {12-15},
  author = {Bolks, A. and DeWire, A. and Harcum, J.B.},
  file = {/Users/Shared/Zotero/storage/WR5TWAQH/tech_notes_10_jun2014_r.pdf}
}

@article{dongPrincipledMissingData2013,
  langid = {english},
  title = {Principled {{Missing Data Methods}} for {{Researchers}}},
  volume = {2},
  issn = {2193-1801},
  url = {http://springerplus.springeropen.com/articles/10.1186/2193-1801-2-222},
  doi = {10.1186/2193-1801-2-222},
  abstract = {The impact of missing data on quantitative research can be serious, leading to biased estimates of parameters, loss of information, decreased statistical power, increased standard errors, and weakened generalizability of findings. In this paper, we discussed and demonstrated three principled missing data methods: multiple imputation, full information maximum likelihood, and expectation-maximization algorithm, applied to a real-world data set. Results were contrasted with those obtained from the complete data set and from the listwise deletion method. The relative merits of each method are noted, along with common features they share. The paper concludes with an emphasis on the importance of statistical assumptions, and recommendations for researchers. Quality of research will be enhanced if (a) researchers explicitly acknowledge missing data problems and the conditions under which they occurred, (b) principled methods are employed to handle missing data, and (c) the appropriate treatment of missing data is incorporated into review standards of manuscripts submitted for publication.},
  number = {1},
  journaltitle = {SpringerPlus},
  urldate = {2018-05-01},
  date = {2013},
  pages = {222},
  keywords = {fmi},
  author = {Dong, Yiran and Peng, Chao-Ying Joanne},
  file = {/Users/Shared/Zotero/storage/9R33BIZR/Dong and Peng - 2013 - Principled missing data methods for researchers.pdf}
}

