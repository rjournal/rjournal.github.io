@article{anderson2017a,
  title = {Black-Swan Events in Animal Populations},
  volume = {114},
  doi = {10.1073/pnas.1611525114},
  abstract = {Black swans are improbable events that nonetheless occur\textemdash{}often with profound consequences. Such events drive important transitions in social systems (e.g., banking collapses) and physical systems (e.g., earthquakes), and yet it remains unclear the extent to which ecological population numbers buffer or suffer from such extremes. Here, we estimate the prevalence and direction of black-swan events (heavy-tailed process noise) in 609 animal populations after accounting for population dynamics (productivity, density dependence, and typical stochasticity). We find strong evidence for black-swan events in $\sim\sim$$<$mml:math xmlns:mml="http://www.w3.org/1998/Math/MathML"$>$$\sim$$<$/mml:mo$>$$<$/mml:math$>$4\% of populations. These events occur most frequently for birds (7\%), mammals (5\%), and insects (3\%) and are not explained by any life-history covariates but tend to be driven by external perturbations such as climate, severe winters, predators, parasites, or the combined effect of multiple factors. Black-swan events manifest primarily as population die-offs and crashes (86\%) rather than unexpected increases, and ignoring heavy-tailed process noise leads to an underestimate in the magnitude of population crashes. We suggest modelers consider heavy-tailed, downward-skewed probability distributions, such as the skewed Student tt$<$mml:math xmlns:mml="http://www.w3.org/1998/Math/MathML"$>$t$<$/mml:mi$>$$<$/mml:math$>$ used here, when making forecasts of population abundance. Our results demonstrate the importance of both modeling heavy-tailed downward events in populations, and developing conservation strategies that are robust to ecological surprises.},
  number = {12},
  journal = {Proceedings of the National Academy of Sciences},
  author = {Anderson, Sean C. and Branch, Trevor A. and Cooper, Andrew B. and Dulvy, Nicholas K.},
  year = {2017},
  keywords = {population dynamics,die-offs,ecological risk,ecological surprises,mass mortality},
  pages = {3252--3257},
  file = {/Users/eric.ward/Zotero/storage/XS6B8PIV/Anderson et al. - 2017 - Black-swan events in animal populations.pdf;/Users/eric.ward/Zotero/storage/4EL6DAQX/3252.html},
  pmid = {28270622}
}

