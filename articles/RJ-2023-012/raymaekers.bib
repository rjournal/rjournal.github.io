@article{10.2307.27639980,
 ISSN = {01621459},
 URL = {http://www.jstor.org/stable/27639980},
 abstract = {We propose weighted repeated median filters and smoothers for robust nonparametric regression in general and for robust online signal extraction from time series in particular. The new methods allow us to remove outlying sequences and to preserve discontinuities (shifts) in the underlying regression function (the signal) in the presence of local linear trends. Suitable weighting of the observations according to their distances in the design space reduces the bias arising from nonlinearities and improves the efficiency using larger bandwidths, while still distinguishing long-term shifts from outlier sequences. Other localized robust regression techniques like S, M, and MM estimators as well as weighted L1 regression, are examined for comparison.},
 author = {Roland Fried and Jochen Einbeck and Ursula Gather},
 journal = {Journal of the American Statistical Association},
 number = {480},
 pages = {1300--1308},
 publisher = {[American Statistical Association, Taylor & Francis, Ltd.]},
 title = {Weighted Repeated Median Smoothing and Filtering},
 urldate = {2022-09-18},
 volume = {102},
 year = {2007}
}



