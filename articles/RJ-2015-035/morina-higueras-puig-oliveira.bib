@article{geyer1994,
  title =	 {On the Asymptotics of Constrained {$M$}-Estimation},
  volume =	 22,
  doi =		 {10.1214/aos/1176325768},
  abstract =	 {Limit theorems for an {MM}-estimate constrained to
                  lie in a closed subset of
                  ℝd{\textbackslash}mathbb\{R\}{\textasciicircum}d are
                  given under two different sets of regularity
                  conditions. A consistent sequence of global
                  optimizers converges under Chernoff regularity of
                  the parameter set. A n‾‾√{\textbackslash}sqrt
                  n-consistent sequence of local optimizers converges
                  under Clarke regularity of the parameter set. In
                  either case the asymptotic distribution is a
                  projection of a normal random vector on the tangent
                  cone of the parameter set at the true parameter
                  value. Limit theorems for the optimal value are also
                  obtained, agreeing with Chernoff's result in the
                  case of maximum likelihood with global optimizers.},
  language =	 {{EN}},
  number =	 4,
